{
  "name": "@netsells/vue-wrapped-styles",
  "version": "0.1.3",
  "description": "Wraps a component to add styles or change the default props",
  "main": "dist/index.js",
  "scripts": {
    "codecov": "codecov",
    "test": "jest",
    "test:mutate": "stryker run",
    "lint": "eslint --max-warnings 0 src spec",
    "docs": "jsdoc src",
    "build": "babel src -d dist",
    "prepublishOnly": "yarn build"
  },
  "author": "Sam Boylett <sam.boylett@netsells.co.uk>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@netsells/eslint-config": "^1.8.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^25.3.0",
    "codecov": "^3.1.0",
    "eslint": "^4.10.0",
    "eslint-plugin-jest": "^22.1.2",
    "express": "^4.16.4",
    "jest": "^25.3.0",
    "jsdoc": "^3.5.5",
    "regenerator-runtime": "^0.13.1",
    "start-server-and-test": "^1.7.11",
    "stryker": "^0.33.1",
    "stryker-api": "^0.22.0",
    "stryker-babel-transpiler": "^0.7.10",
    "stryker-html-reporter": "^0.16.9",
    "stryker-javascript-mutator": "^0.12.1",
    "stryker-jest-runner": "^1.2.9",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netsells/vue-wrapped-styles.git"
  },
  "jest": {
    "transform": {
      "\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^~/(.+)$": "<rootDir>/src/$1"
    },
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
